{
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 120,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": "*.html",
      "options": { "parser": "angular" }
    }
  ],
  "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-tailwindcss"]
}
